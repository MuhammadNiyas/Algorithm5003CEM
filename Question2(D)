graph = Graph()
for uid in users:
    graph.addVertex(uid)

# Example follow relationships
graph.addEdge(1, 2)  # Ali follows Aisyah
graph.addEdge(1, 3)  # Ali follows John
graph.addEdge(2, 4)  # Aisyah follows Mei
graph.addEdge(3, 1)  # John follows Ali
graph.addEdge(4, 5)  # Mei follows Zara
graph.addEdge(5, 2)  # Zara follows Aisyah
