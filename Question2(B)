class Person:
    def __init__(self, user_id, name, gender, bio, privacy):
        self.user_id = user_id
        self.name = name
        self.gender = gender
        self.bio = bio
        self.privacy = privacy  # "public" or "private"

    def display_profile(self):
        if self.privacy == "public":
            return (f"Name: {self.name}\nGender: {self.gender}\n"
                    f"Bio: {self.bio}\nPrivacy: {self.privacy}")
        else:
            return f"Name: {self.name}\nPrivacy: {self.privacy}"
